//////////////////////////////땅의 범위를 벗어나면 죽게되는 die_X()함수///////////////////////////

function die_X()	//	땅의 범위 왼쪽, 오른쪽을 벗어나면 떨어지면서  죽게됨
{
	if((100+dx)<48){
		ctx.clearRect(100+dx,1318+dy,70,95);
		ctx2.clearRect(100+dx,1318+dy,70,95);

		dy++;
		ctx.drawImage(img_man,100+dx,1318+dy,70,95);

	}
	else if((100+dx)>=930){

		ctx.clearRect(100+dx,1318+dy,70,95);
		ctx2.clearRect(100+dx,1318+dy,70,95);
		dy++;
		ctx.drawImage(img_man,100+dx,1318+dy,70,95);

	}

	if(((1500-wy)-(1318+dy+80))<0)	// 캐릭터가 떨어지다가 정해진 좌표에 도달하면 ending() 호출
	{
		clearInterval(tev_die);
		ending();
	}
}

