//////////////////////////////////////////////////////////////////////////////////////////////////
///////////////////////////특정 키 누르면 게임화면 사라지는  jump()함수///////////////////////////

function jump(event){		//space bar가 눌리면 게임화면 사라짐
	var keyCode=0;

	if(event==null){
		keyCode=window.event.keyCode;
		window.event.preventDefault();
	}
	else{
		keyCode=event.keyCode;
		event.preventDefault();
	}

	if(keyCode==32){			//space bar의 키코드는 32. space bar가 눌리면  clear_water()로 물 멈춰주고
		clear_water();
		clearInterval(tev1);

		clearInterval(tev3);
		clearInterval(tevbgm);

		ctx.clearRect(0,0,1000,1712);		//게임화면 지워짐
		ctx1.clearRect(0,0,1000,1712);
		ctx2.clearRect(0,0,1000,1712);
		ctx3.clearRect(0,0,1000,1712);
		ctx4.clearRect(0,0,1000,1712);

		myAudio1.pause();		//배경음악 멈춤
		myAudio1.currentTime=0;



	}
}

/***********************************jump()함수 끝***********************************/



//////////////////////////////////////////////////////////////////////////////////////////////////
///////////////////////특정 키 누르면 다음 라운드로 넘어가는 cheat_key()함수//////////////////////


function cheat_key(event){		//넘버패드 0을 누르면 다음 라운드로 넘어감
	var keyCode=0;
	if(event==null){
		keyCode=window.event.keyCode;
		window.event.preventDefault();
	}
	else{
		keyCode=event.keyCode;
		event.preventDefault();
	}

	if(keyCode==96){			//넘버패드 0의 키코드는 96
		cheat=true;
	}
}


/***********************************cheat_key()함수 끝***********************************/



