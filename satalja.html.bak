//////////////////////////////////////////////////////////////////////////////////////////////////
//////////////////////////////캐릭터의 움직임에 관한 getkeyAndMove()함수//////////////////////////

function getkeyAndMove(event){
	mycanvas2 = document.getElementById('canvas2');
	ctx2 = document.getElementById('canvas2').getContext("2d");

	var keyCode;
	if(((100+dx)<48)||((100+dx)>=930))				//땅의 범위 넘어가면 떨어져서 죽도록
	{
		tev_die=setInterval("die_X();",80);
	}

	if(event==null){
		keyCode=window.event.keyCode;
		window.event.preventDefault();
	}
	else{
		keyCode=event.keyCode;
		event.preventDefault();
	}

	switch(keyCode){

	case 37: //왼쪽화살표

		var possible_width=true;
		for(var i=1; i<12;i++)
		{
			if(205+110*(i-1)<=1318+dy+95 && 1318+dy+95<=198+110*i)  //땅이 아닌 사다리 위에서는 false로 , 이미지의 아래 기준
				possible_width=false;
		}

		//맨 윗사다리에서는 왼쪽 못움직인다
		if((1318+dy + 95 - 20) - 10 < 200)
			posible_width=false;

		if(possible_width)
		{	
			ctx2.clearRect(100+dx,1318+dy,70,95);
			dx=dx-10;


			//달리는 모션

			if(left_walk%8 == 1)
				ctx2.drawImage(img_man_L1,100+dx,1318+dy,70,95);

			if(left_walk%8 == 2)
				ctx2.drawImage(img_man_L2,100+dx,1318+dy,70,95);

			if(left_walk%8 == 3)
				ctx2.drawImage(img_man_L1,100+dx,1318+dy,70,95);

			if(left_walk%8 == 4)
				ctx2.drawImage(img_man_L3,100+dx,1318+dy,70,95);

			if(left_walk%8 == 5)
				ctx2.drawImage(img_man_L4,100+dx,1318+dy,70,95);

			if(left_walk%8 == 6)
				ctx2.drawImage(img_man_L5,100+dx,1318+dy,70,95);

			if(left_walk%8 == 7)
				ctx2.drawImage(img_man_L4,100+dx,1318+dy,70,95);

			if(left_walk%8 == 0)
				ctx2.drawImage(img_man_L3,100+dx,1318+dy,70,95);

			left_walk++;

		}
		break;

	case 38: //위쪽화살표

		nox_num=0;
		var possible_height=false;
		var i1=0;
		var i2=0;

		for(i1=200;i1<1410;i1+=110)
		{
			for(i2=0;i2<nox[nox_num];i2++)
			{

				//가로와 세로 둘다 따져줌
				//미리 움직일 곳 검사해주었음.
				if(ladderlisty[i1][i2] >= 100+dx+10 && ladderlisty[i1][i2] <= 100+dx+35 && i1 <= (1318+dy + 95) -10 && i1+110 >= (1318+dy + 95 - 20) - 10)
				{ 
					ctx2.clearRect(100+dx,1318+dy,70,95);
					dy=dy-10;


					if(up_walk%8 == 1 || up_walk%8 == 2 || up_walk%8 == 3 || up_walk%8 == 4)
						ctx2.drawImage(img_man_U1,100+dx,1318+dy,70,95);

					if(up_walk%8 == 5 ||up_walk%8 == 6 ||up_walk%8 == 7 ||up_walk%8 == 0)
						ctx2.drawImage(img_man_U2,100+dx,1318+dy,70,95);
					up_walk++;
				}

			}
			nox_num++;
		}


		//맨 위 사다리위에 있는지 검사
		if(563 >= 100+dx+10 && 563 <= 100+dx+35 && 88 <= (1318+dy + 95) -10 && 200 >= (1318+dy + 95 - 20) - 10)
		{ 
			ctx2.clearRect(100+dx,1318+dy,70,95);
			dy=dy-10;
			ctx2.drawImage(img_man,100+dx,1318+dy,70,95);
		}

		break;


	case 39: //오른쪽화살표

		var possible_width=true;
		for(var i=0; i<12;i++)
		{
			if(205+110*(i-1)<=1318+dy+95 && 1318+dy+95<=198+110*i)
				possible_width=false;
		}

		//맨 윗사다리에서는 오른쪽 못움직인다
		if((1318+dy + 95 - 20) - 10 < 200)
			poosible_width=false;

		if(possible_width)
		{	
			ctx2.clearRect(100+dx,1318+dy,70,95);
			dx=dx+10;


			//달리는 모션

			if(right_walk%8 == 1)
				ctx2.drawImage(img_man_R1,100+dx,1318+dy,70,95);

			if(right_walk%8 == 2)
				ctx2.drawImage(img_man_R2,100+dx,1318+dy,70,95);

			if(right_walk%8 == 3)
				ctx2.drawImage(img_man_R1,100+dx,1318+dy,70,95);

			if(right_walk%8 == 4)
				ctx2.drawImage(img_man_R3,100+dx,1318+dy,70,95);

			if(right_walk%8 == 5)
				ctx2.drawImage(img_man_R4,100+dx,1318+dy,70,95);

			if(right_walk%8 == 6)
				ctx2.drawImage(img_man_R5,100+dx,1318+dy,70,95);

			if(right_walk%8 == 7)
				ctx2.drawImage(img_man_R4,100+dx,1318+dy,70,95);

			if(right_walk%8 == 0)
				ctx2.drawImage(img_man_R3,100+dx,1318+dy,70,95);
			right_walk++;

		}
		break;



	case 40: //아래쪽화살표
		nox_num=0;
		var possible_height=false;
		var i1=0;
		var i2=0;

		for(i1=200;i1<1410;i1+=110)
		{
			for(i2=0;i2<nox[nox_num];i2++)
			{
				//가로와 세로 둘다 따져줌
				//움직일 곳 미리 검사
				if(ladderlisty[i1][i2] >= 100+dx && ladderlisty[i1][i2] <= 100+dx+70 && i1 <= (1318+dy + 95) + 10 && i1+110 >= (1318+dy + 95 - 20) + 20)
				{ 
					ctx2.clearRect(100+dx,1318+dy,70,95);
					dy=dy+10;

					if(down_walk%8 == 1 || down_walk%8 == 2 || down_walk%8 == 3 || down_walk%8 == 4)
						ctx2.drawImage(img_man_D1,100+dx,1318+dy,70,95);

					if(down_walk%8 == 5 || down_walk%8 == 6 || down_walk%8 == 7 ||down_walk%8 == 0)
						ctx2.drawImage(img_man_D2,100+dx,1318+dy,70,95);

					down_walk++;
				}

			}
			nox_num++;
		}
		break;


	default:
		window.removeEventListener('keydown',getkeyAndMove,false);
	}
}

/***********************************getkeyAndMove()함수 끝***********************************/



