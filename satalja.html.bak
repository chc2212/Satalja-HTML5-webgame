function draw_fire(){ 
	mycanvas = document.getElementById('canvas');
	ctx = document.getElementById('canvas').getContext("2d");


	for(var i=0 ; i < fireList.size() ; i++){
		ctx.clearRect(0,1512,1000,100);
		ctx.clearRect(fireList.get(i).x-25,fireList.get(i).y-55,50,50); // 이미지잔상 지우기 위함
		ctx.drawImage( fireImg[ fireList.get(i).img ], fireList.get(i).x-25, fireList.get(i).y-25, 50, 50 );

		ctx.restore();
	}
	//불호령움직이기
	movefire();
}
