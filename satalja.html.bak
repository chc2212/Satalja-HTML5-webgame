///////////////////////////////불호령에 관한 처리를 하는 movefire()함수///////////////////////////


function movefire(){		//불호령의 움직임에 대한 처리를 하는 함수

	for(var i=fireList.size()-1 ; i>=0 ; i--){

		if(fireList.get(i).isdead){

			fireList.remove(i);//isdead가 true면 불호령을 제거
		}

		//불호령을 움직인다.
		fireList.get(i).y += speed;


		//화면 경계를 벗어나면 제거하기 위해

		if(fireList.get(i).y > 1552){

			fireList.remove(i);

		}


	}

	//일정 확률로 불호령이 만들어지도록 한다.(1/200의 확률)

	var ranNum = Math.floor(Math.random()*count);

	if(ranNum == 1) makefire();

}
/***********************************movefire()함수 끝***********************************/



//////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////불호령 개수와 발생위치에 관한 makefire()함수//////////////////////////


function makefire(){		//불호령의 개수와 발생위치를 랜덤하게 만들어주는 함수


	//불호령의 이미지를 랜덤하게 하기 위해서

	var imgIndex=Math.floor(Math.random()*8);//0-1랜덤한수 발생(닭고르기)

	//불호령의 처음 x좌표를 랜덤하게 하기 위해서

	var fireX=Math.round(Math.random()*950)+25;

	var fireY=0;

	var fire={ x : fireX, y : fireY, img : imgIndex, isDead : false, energy : 100};

	fireList.add(fire);
}

/***********************************makefire()함수 끝***********************************/



//////////////////////////////////////////////////////////////////////////////////////////////////
//////////////////////////불호령과 캐릭터 간 충돌 검사하는 check_fire()함수///////////////////////


function check_fire(){		//캐릭터와 불호령 간의 충돌검사

	for(var i = fireList.size()-1 ; i >= 0 ; i--){

		var minX = fireList.get(i).x-25;

		var maxX = fireList.get(i).x+25;

		var minY = fireList.get(i).y-25;

		var maxY = fireList.get(i).y+25;


		//캐릭터좌표 100+dx,1318+dy,70,95
		var char_x_center = (100+dx+100+dx+70 )/2;  
		var char_y_center = (1318+dy+1318+dy+95)/2;

		if((1318+dy + 95 - 20) - 10 > 170) //맨 위에서는 안죽는다.
		{
			if(char_x_center >= minX &&  char_x_center <= maxX &&  char_y_center >= minY  && char_y_center <= maxY)
			{
				ending();  //캐릭터의 중점이 불호령 그림안에 들어오면 엔딩호출
			}
		}

	}


}

/***********************************check_fire()함수 끝***********************************/


