function intro()
{
	intro_ctx = document.getElementById('canvas5').getContext('2d'); //canvas5로 설정
	tev_girl=setInterval("draw_girl();",4);	//tev_girl <- 차후 clearInterval을 위함.
	tev_man=setInterval("draw_man();",4);	//tev_man <- 차후 clearInterval을 위함.


	canvas5.addEventListener ("click", function (event)  {	// 클릭 이벤트 <스타트버튼>

		if(start_game==false)
		{
			if(girlx==600) //girlx사진이 600이내로 오면
			{
				if(event.x < 1120 - 180 + 20 + 160&& event.x > 970 - 180 + 20 && event.y > 80 && event.y <120) // start버튼입력
				{
					intro_ctx.clearRect(0,0,999,999);
					start_game=true;//기본 false, true이면 실행
					window.clearTimeout(clr1);// draw_girl(), draw_man()의 대화 애니메이션 종료
					window.clearTimeout(clr2);// 말풍선 애니메이션 실행 시 스타트 버튼 클릭하면 애니메이션이 종료 되고 게임실행
					window.clearTimeout(clr3);
					window.clearTimeout(clr4);
					window.clearTimeout(clr5);
					window.clearTimeout(clr6);


					intro_ctx.drawImage(rimg1,-20,180,1020,700);
					setTimeout("intro_ctx.clearRect(-20,180,1020,700);",2000);
					setTimeout("init();",2100); //게임 시작

